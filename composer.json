{
    "name": "taylornetwork/make-html",
    "description": "A trait for to convert plain text to html, inlcuding links.",
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.*",
        "taylornetwork/laravel-helpers": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "dev-master"
    },
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "autoload": {
    	"psr-4": {
    		"TaylorNetwork\\MakeHTML\\": "src/",
            "TaylorNetwork\\MakeHTML\\Tests\\": "tests/"
    	}
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaylorNetwork\\MakeHTML\\MakeHTMLServiceProvider"
            ]
        }
    }
}
